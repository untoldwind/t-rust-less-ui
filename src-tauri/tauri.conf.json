{
  "productName": "t-rust-less-ui",
  "version": "0.2.15",
  "identifier": "io.github.untoldwind",
  "build": {
    "frontendDist": "../app",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "yarn dev:app",
    "beforeBuildCommand": "yarn build:app"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "windows": {
      "certificateThumbprint": "438a68db3bb3fc6dccd8b484f85b0f659db3eed1",
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com"
    },
    "android": {
      "minSdkVersion": 29
    }
  },
  "app": {
    "windows": [
      {
        "title": "T-Rust-Less",
        "width": 1024,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'",
      "capabilities": ["main-window"]
    }
  }
}
