{
    "name": "t-rust-less-ui",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build:app": "webpack --config webpack.config.js --mode production",
        "build:app-dev": "webpack --config webpack.config.js --mode development",
        "build:neon": "electron-build-env neon build --release",
        "dev": "webpack-dev-server --config webpack.config.js --hot --inline --mode development",
        "electron": "electron app",
        "format": "tsfmt -r && scssfmt --recursive sass/**/*.scss && tslint -c tslint.json --project .",
        "clean": "rimraf dist app",
        "dist": "npm run build:neon && npm run build:app && electron-builder"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@fortawesome/fontawesome-free": "^5.13.1",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/mousetrap": "^1.6.3",
        "@types/qrcode.react": "^1.0.1",
        "@types/react": "^16.9.41",
        "@types/react-dom": "^16.9.8",
        "awesome-typescript-loader": "^5.2.1",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^3.6.0",
        "electron": "9.3.0",
        "electron-build-env": "^0.2.0",
        "electron-builder": "^22.7.0",
        "electron-devtools-installer": "^3.1.0",
        "extract-loader": "^5.1.0",
        "file-loader": "^6.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "native-ext-loader": "^2.3.0",
        "neon-cli": "^0.4.0",
        "sass": "^1.26.9",
        "sass-bem": "^2.6.5",
        "sass-loader": "^9.0.1",
        "scssfmt": "^1.0.7",
        "tslint": "^6.1.2",
        "typescript": "^3.9.6",
        "typescript-formatter": "^7.2.2",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "write-file-webpack-plugin": "^4.5.0"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.29.0",
        "@blueprintjs/icons": "^3.19.0",
        "@blueprintjs/select": "^3.13.4",
        "@xstate/react": "^1.0.0-rc.5",
        "decko": "^1.2.0",
        "emotion": "^10.0.27",
        "moment": "^2.27.0",
        "qrcode.react": "^1.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "xstate": "^4.11.0"
    },
    "build": {
        "extraFiles": "*.dll"
    }
}
